{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      "@icons/*": ["./src/assets/icons/*"],
      "@icons_f/*": ["./src/assets/icons/flags/*"],
      "@icons_s/*": ["./src/assets/icons/skills/*"],
      "@imgs/*": ["./src/assets/imgs/*"],
      "@c/*": ["./src/components/*"],
      "@l/*": ["./src/layouts/*"],
      "@p/*": ["./src/pages/*"],
      "@u/*": ["./src/utils/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}